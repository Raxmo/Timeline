<Window x:Class="Timeline.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Timeline"
        mc:Ignorable="d"
        SizeChanged="Window_SizeChanged"
        MouseWheel="Window_MouseWheel"
        Title="MainWindow" Height="450" Width="800" Background="#FF1A1A1A">
    <Grid>
        <Canvas ClipToBounds="True" Loaded="TLPan_Loaded" Name="TLPan" Margin="10,50,10,100" VerticalAlignment="Top" Height="30" Background="#FF24266F">
        </Canvas>
    </Grid>
</Window>
